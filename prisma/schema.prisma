// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}


model Libri {
  id Bytes @id @default(auto()) @map("_id") @db.ObjectId 
  titolo String
  autore String
  urlImage String?
}